{
    "rewrites": [
        {
            "source": "/api/uaf/:path*",
            "destination": "/api/proxy"
        },
        {
            "source": "/api/legacy/:path*",
            "destination": "/api/proxy"
        },
        {
            "source": "/((?!assets/|.*\\..*).*)",
            "destination": "/index.html"
        }
    ]
}